{
  "plugins": [
    "react-hooks", "jsx-a11y"
  ],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "jsx-a11y/no-distracting-elements": [
      2,
      {
        "elements": [
          "marquee",
          "blink"
        ]
      }
    ]
  },
  "parser": "babel-eslint",
  "parserOptions": { 
    "ecmaVersion": 6,
    "sourceType": "module",
    "parser": "vue-eslint-parser"
 } 
}